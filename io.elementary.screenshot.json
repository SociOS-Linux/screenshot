{
    "app-id": "io.elementary.screenshot",
    "base": "io.elementary.BaseApp",
    "base-version": "juno-19.08",
    "runtime": "org.gnome.Platform",
    "runtime-version": "3.34",
    "sdk": "org.gnome.Sdk",
    "command": "io.elementary.screenshot-tool",
    "finish-args": [
        "--share=ipc",
        "--socket=fallback-x11",
        "--talk-name=org.gnome.SessionManager",
        "--metadata=X-DConf=migrate-path=/io/elementary/screenshot-tool/"
    ],
    "cleanup": [
        "/include",
        "/lib/pkgconfig",
        "/man",
        "/share/doc",
        "/share/gtk-doc",
        "/share/man",
        "/share/pkgconfig",
        "/share/vala",
        "*.la",
        "*.a"
    ],
    "modules": [{
        "name": "screenshot",
        "buildsystem": "meson",
        "sources": [{
            "type": "dir",
            "path": "."
        }]
    }]
}
